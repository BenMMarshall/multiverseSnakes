@article{Kay2016,
	title = {Effects of past and present livestock grazing on herpetofauna in a landscape-scale experiment},
	volume = {31},
	issn = {15231739},
	doi = {10.1111/cobi.12779},
	abstract = {Livestock grazing is the most widespread land-use on Earth, and can have negative effects on biodiversity. Yet, many of the mechanisms by which grazing leads to changes in biodiversity remain unresolved. One reason is that conventional grazing studies often target broad treatments rather than specific parameters of grazing (e.g. intensity, duration and frequency), or fail to account for historical grazing effects. We designed a landscape-scale replicated grazing experiment (15,000 km(2) , 97 sites) to examine the impact of: (i) past grazing management and (ii) present grazing parameters (intensity, duration and frequency) on a community of ground-dependent herpetofauna (39 species). We conducted analyses of community variables (richness and composition) using the full species dataset and multi-season patch-occupancy models (examining local colonization and extinction) for the seven most abundant species. We found past grazing practice did not influence community richness but did affect community composition and patch colonization and extinction for four of seven species. Present grazing parameters did not influence community richness or composition, but six of the seven target species were affected by at least one grazing parameter. Grazing frequency had the most consistent influence, positively affecting three of seven species (increased colonization or decreased extinction). Our study reveals effects of past and present grazing on herpetofauna that have previously been overlooked, providing the following implications for conserving herpetofauna in grazing landscapes: 1) past grazing practice influences community composition and population dynamics in some species in different ways, suggesting conservation programs should target a mosaic of grazing history; 2) species respond differently to specific current grazing practices requiring incentive programs that target a diversity of approaches rather than focussing on a change such as reduced intensity; 3) gathering detailed grazing data to allow determination of fine-scale grazing attributes is essential for advancing grazing as a conservation management strategy. This article is protected by copyright. All rights reserved.},
	number = {2},
	journal = {Conservation Biology},
	author = {Kay, Geoffrey M. and Mortelliti, Alessio and Tulloch, Ayesha and Barton, Philip and Florance, Daniel and Cunningham, Saul A. and Lindenmayer, David B.},
	year = {2016},
	pmid = {27314208},
	keywords = {reptile, amphibian, natural resource management, anfibio, box gum grassy woodland, community, comunidad, grazing legacy, grazing regime, legado de pastoreo, manejo de recursos naturales, mecanismo, mechanism, pastizal boscoso de eucalipto, régimen de pastoreo, reptil, southeast Australia, sureste de Australia},
	pages = {446--458},
}

@article{Bauder2016a,
	title = {Seasonal {Variation} in {Eastern} {Indigo} {Snake} ({Drymarchon} couperi) {Movement} {Patterns} and {Space} {Use} in {Peninsular} {Florida} at {Multiple} {Temporal} {Scales}},
