	file = {Griffith - 2019 - Cooperation and Coordination in Socially Monogamou.pdf:D\:\\Documents\\Work Files\\Zotero Library\\storage\\FABUER4B\\Griffith - 2019 - Cooperation and Coordination in Socially Monogamou.pdf:application/pdf},
